#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
    int X;
    int N;
    int price=0;
    int count=0;
    int Y = 0;

    scanf("%d", &X);
    scanf("%d", &N);

    for (int i = 0; i < N; i++) {
        scanf("%d %d", &price, &count);
        Y += price * count;
    }
    
    if (Y == X) {
        printf("Yes");
    }
    else {
        printf("No");
    }
    return 0;
}
