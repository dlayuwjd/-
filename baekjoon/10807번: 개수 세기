#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
    int N[100];
    int n;
    int num;
    int count = 0;

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%d", &N[i]);
    }

    scanf("%d", &num);

    for (int i = 0; i < n; i++) {
        if (N[i] == num) {
            count++;
        }
    }
    printf("%d", count);

    return 0;
}
